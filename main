#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main()
{
    int m, n; cin >> m >> n;
    vector<int> nums;
    for(int i = 0; i < m*n; i++)
    {
        int tmp; cin >> tmp;
        nums.push_back(tmp);
    }
    sort(nums.begin(), nums.end());
    vector<vector<int>> matrix(m, vector<int>(n));
    int a = 0, b = 0;
    int c = m - 1; 
    int d = n - 1;
    int index = 0;
    while(a <= c && b <= d)
    {
        if(a == c)
        {
            for(int i = b; i<=d; i++)
            {
                matrix[a][i] = nums[index++];
            }
        }
        else if( b == d)
        {
            for(int i = a; i<=c; i++)
            {
                matrix[i][b] = nums[index++];
            }
        }
        else
        {
            for(int i = b; i<d; i++)
            {
                matrix[a][i] = nums[index++];
            }
            for(int j = a; j<c; j++)
            {
                matrix[j][d] = nums[index++];
            }
            for(int i = d; i>b; i--)
            {
                matrix[c][i] =  nums[index++];
            }
            for(int j = c; j>a; j--)
            {
                matrix[j][a] =  nums[index++];
            }
        }  
        a++;
        b++;
        c--;
        d--;
    } 
    for(int i = 0; i < m; i++)
    {
        for(int j = 0; j < n; j++)
        {
            cout << matrix[i][j] << " ";
        }
        cout << endl;
    }
    return 0;
}
